{% extends "base.html.twig" %}
{% block body %}
    <div class="card">
        {{ include('Household/householdHeading.html.twig') }}
        <div class="card-body">
            <form id="hosuehold_edit" action="{{ path('household_edit', { 'id': household.id }) }}" method="post">
                {{ include('Member/memberShowBlock.html.twig') }}
                {{ include('Address/addressBlock.html.twig') }}
                {{ include('Household/householdEditBlock.html.twig') }}
                {{ form_row(form._token) }}
                <div align="center"><input class="btn-sm btn-info" type="submit" value="Submit">
                    <input class="btn-sm btn-info" type="reset"></div>
            </form>
        </div>
    </div>
{% endblock %}
{% block javascripts %}
    {{ parent() }}
    {{ encore_entry_script_tags('household') }}
{% endblock %}
