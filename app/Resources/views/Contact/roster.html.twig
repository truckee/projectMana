<pdf>
    <dynamic-page>
        <placeholders>
            <header>
                <div height="110px" width="100%">
                    <div text-align="center" font-size="14"><p><b>{{source}} Contacts for {{ center }} on {{ date|date('m/d/Y') }}</b></p></div>
                </div>
            </header>
            <footer>
                <div height="20px" text-align="center">
                    <page-info offset="0" format="Page %s of %s" />
                </div>
            </footer>
        </placeholders>
        <div margin="0 100">
            <table class="contact">
                <tr>
                    <td></td>
                    <td>Name</td>
                    <td>ID</td>
                    <td>DOB</td>
                </tr>
                {% set testId = '' %}
                {% for contact in contacts %}
                    {% if contact.household.id != testId %}
                        <tr>
                            <td width="20"><img src="{{ pdf_image('unchecked.png') }}" /></td>
                            <td>{{ contact.household.head.sname }}, {{ contact.household.head.fname }}</td>
                            <td>{{ contact.household.id }}</td>
                            <td>{% if contact.household.head.dob is not empty %}{{ contact.household.head.dob|date('m/d/Y') }}{% else %}___________{% endif %}</td>
                        </tr>
                        {% set testId = contact.household.id %}
                    {% endif %}
                {% endfor %}
            </table>
            <br />

        </div>
    </dynamic-page>
</pdf>