<div class="panel panel-info">
    <div class="panel-heading">Household<br>
    </div>
    <div class="panel-body">
        <div class="row">
            <div class="col-md-2">
                {{ form_row(form.active) }}
            </div>
            <div class="col-md-2">
                {{ form_row(form.areacode) }}
            </div>
            <div class="col-md-2">
                {{ form_row(form.phoneNumber) }}
            </div>
            <div class="col-md-2">
                {{ form_row(form.center) }}
            </div>
        </div>
        <div class="row">
            {# Q4 Benefits #}
            <div class="col-md-3">
                {{ form_row(form.benefits) }}
            </div>
            {# Q5 Why not foodstamps #}
            <div class="col-md-3">
                {{ form_row(form.notfoodstamp, {'attr': {'class':'notfoodstamp'}}) }}
            </div>
            {# Q6 Income #}
            <div class="col-md-2">
                {{ form_row(form.income) }}
            </div>
            {# Q7 Housing #}
            <div class="col-md-2">
                {{ form_row(form.housing) }}
            </div>
        </div>
        <div class="row">
            <div class="col-md-12"><strong>Check all that apply for each column below</strong><hr></div>
        </div>
        <div class="row">
            {# Q8 Insufficient food #}
            <div class="col-md-4">
                {{ form_row(form.reasons) }}
            </div>
            {# Q9 Seeking services #}
            <div class="col-md-4">
                {{ form_row(form.assistances) }}
                <div class="row" id="seeking">
                    <div class="col-md-12">{{ form_row(form.seeking) }}</div>
                </div>
            </div>
            {# Q9 Receiving services #}
            <div class="col-md-4">
                {{ form_row(form.organizations) }}
                <div class="row" id="receiving">
                    <div class="col-md-12">{{ form_row(form.receiving) }}</div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-2">
                {{ form_row(form.compliance) }}
            </div>
            <div class="col-md-2">
                {{ form_row(form.complianceDate) }}
            </div>
            <div class="col-md-2">
                {{ form_row(form.shared) }}
            </div>
            <div class="col-md-2">
                {{ form_row(form.sharedDate) }}
            </div>
        </div>
    </div>
    <div id="household_options" style="display:none;">{{ metadata }}</div>
</div>
