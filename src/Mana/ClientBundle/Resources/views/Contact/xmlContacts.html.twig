<root>
{% for key, contact in contacts %}
<contact>{{ key }}
    <id>{{ key }}</id>
    <center>{{ contact.center }}</center>
    <hoh>{{ contact.head }}</hoh>
    <dob>{{ contact.dob|date('m/d/Y') }}</dob>
    <type>{{ contact.type }}</type>
    <date>{{ contact.date }}</date>
</contact>>
{% endfor %}
</root>
