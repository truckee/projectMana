
{% extends 'ManaClientBundle:Default:layout.html.twig' %}


{% block body -%}
<div class="title">Site</div>
<div class="width40">
    
    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Site</th>
                <td>{{ entity.center }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('center') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('center_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
</ul>
    </div>
{% endblock %}
