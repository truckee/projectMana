
{% extends 'ManaClientBundle:Default:layout.html.twig' %}


{% block body -%}
<div class="title">Specialneed</div>
<div class="width40">
    
    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Need</th>
                <td>{{ entity.need }}</td>
            </tr>
            <tr>
                <th>Order</th>
                <td>{{ entity.order }}</td>
            </tr>
            <tr>
                <th>Enabled</th>
                <td>{% if entity.enabled %}Yes{%else%}No{%endif%}</td>
            </tr>
         </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('specialneed') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('specialneed_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
</ul>
    </div>
{% endblock %}
