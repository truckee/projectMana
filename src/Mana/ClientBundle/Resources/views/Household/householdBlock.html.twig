<div class="section">Household</div>
{#{{ errorString }}#}
<ul class="formblock">
    <li><b>ID:</b> {{ household.id }}</li>
    <li id="dateAdded"><b>Date added:</b> {% if household.dateAdded is not null %}{{ household.dateAdded|date('m/d/Y') }}<input type="hidden" name="household[dateAdded]" value="{{ household.dateAdded|date('m/d/Y') }}">{% endif %}</li>
    <li><b>Active?:</b> {{ form_widget(form.active, {'attr': {'class':'smallform'}}) }}</li>
{#    <li><b>Pregnancy<br>in household?:</b>{{ form_widget(form.pregnant, {'attr': {'class':'smallform'}}) }}</li>
    <li><b>Special needs<br>in household?: </b>{{ form_widget(form.specialneed, {'attr': {'class':'smallform'}}) }}</li>
    <li><b>WIC: </b>{{ form_widget(form.wic, {'attr': {'class':'smallform'}}) }}</li>#}
    <li><b>Phone: </b>{% for phone in form.phones %}({{ form_widget(phone.areacode, {'attr': {'class':'smallform'}}) }}) {{ form_widget(phone.phoneNumber, {'attr': {'class':'smallform'}}) }}{% endfor %}</li>
    <li><b>Food stamps?</b>{{ form_widget(form.foodStamps, {'attr': {'class':'smallform'}}) }} 
    {{ form_label(form.fsamount) }}{{ form_widget(form.fsamount, {'attr': {'class':'smallform'}}) }}
    {{ form_label(form.notfoodstamp) }}{{ form_widget(form.notfoodstamp, {'attr': {'class':'smallform'}}) }} 
    </li>
    <li><b>{{ form_label(form.incomeSource) }}: </b>{{ form_widget(form.incomeSource, {'attr': {'class':'smallform-checkbox'}}) }}</li>
    <li><b>Income: </b>{{ form_widget(form.income, {'attr': {'class':'smallform'}}) }}</li>
    <li><b>Housing: </b>{{ form_widget(form.housing, {'attr': {'class':'smallform'}}) }}</li>
    <li><b>{{ form_label(form.appliances) }}: </b>{{ form_widget(form.appliances, {'attr': {'class':'smallform-checkbox'}}) }}</li>
    <li><b>{{ form_label(form.reasons) }}: </b>{{ form_widget(form.reasons, {'attr': {'class':'smallform-checkbox'}}) }}</li>
    <li></li>
</ul>
